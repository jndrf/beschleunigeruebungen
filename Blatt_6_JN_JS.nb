 CellChangeTimes->{{3.720780666657001*^9, 3.720780671208075*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"bfield", "=", 
  RowBox[{
   RowBox[{"Curl", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"hr", ",", "h\[Theta]", ",", "hz"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"r", ",", "\[Theta]", ",", "z"}], "}"}], ",", 
     "\"\<Cylindrical\>\""}], "]"}], "\[Equal]", " ", 
   RowBox[{"\[Epsilon]", "*", 
    RowBox[{"D", "[", 
     RowBox[{"efield", ",", 
      RowBox[{"{", 
       RowBox[{"t", ",", "1"}], "}"}]}], "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.720856055213079*^9, 3.720856104397645*^9}, {
  3.720856139844983*^9, 3.7208561757144823`*^9}, {3.720856277565382*^9, 
  3.7208563293471937`*^9}, {3.720856901375639*^9, 3.7208569137651987`*^9}, {
  3.720856983797014*^9, 3.720856990252387*^9}, {3.7208570593969584`*^9, 
  3.72085708997174*^9}, {3.720857498000897*^9, 3.7208575037907047`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"{", 
   RowBox[{"0", ",", "0", ",", 
    FractionBox["h\[Theta]", "r"]}], "}"}], "\[Equal]", 
  RowBox[{"{", 
   RowBox[{"0", ",", "0", ",", 
    RowBox[{
     RowBox[{"-", "e0"}], " ", "\[Epsilon]", " ", "\[Omega]", " ", 
     RowBox[{"Sin", "[", 
      RowBox[{"t", " ", "\[Omega]"}], "]"}]}]}], "}"}]}]], "Output",
 CellChangeTimes->{{3.720856302671517*^9, 3.720856329839779*^9}, {
   3.720856476595661*^9, 3.7208564797907248`*^9}, 3.720856517364064*^9, 
   3.720856554603648*^9, 3.720856914938388*^9, 3.720857040784225*^9, {
   3.7208570907642927`*^9, 3.720857104406474*^9}, {3.72085718798578*^9, 
   3.720857204731145*^9}, {3.7208575045690317`*^9, 3.720857533722991*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{"{", "bfield", "}"}], ",", "h\[Theta]"}], "]"}]], "Input",
