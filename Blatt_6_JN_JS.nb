Notebook[{

Cell[CellGroupData[{
Cell["Blatt 6 - Jonas Neundorf & Jan Skottke", "Title",
 CellChangeTimes->{{3.720777086091834*^9, 3.7207770956600533`*^9}}],

Cell[CellGroupData[{

Cell["Der \[OpenCurlyDoubleQuote]pill-box\[CloseCurlyDoubleQuote] Resonator", \
"Section",
 CellChangeTimes->{{3.7207771043070183`*^9, 3.720777112899365*^9}, {
  3.720781633138386*^9, 3.7207816369732227`*^9}}],

Cell["\<\
Aufgrund der Zylindersymmetrie des Resonators bietet es sich an, \
Zylinderkoordinaten zu benutzen.\
\>", "Text",
 CellChangeTimes->{{3.720781642020694*^9, 3.720781656941358*^9}, {
  3.720781713142508*^9, 3.720781725860688*^9}}],

Cell[CellGroupData[{

Cell["\<\
Berechnung des  allgemeinen elektrischen Feldes in der TM010 Mode\
\>", "Subsection",
 CellChangeTimes->{{3.720777117819276*^9, 3.720777148514091*^9}}],

Cell["Lege einige Konstanten fest:", "Text",
 CellChangeTimes->{{3.72077721896183*^9, 3.7207772264096518`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"constants", " ", "=", " ", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"c", " ", "\[Rule]", " ", 
      RowBox[{"3.", "*", 
       SuperscriptBox["10", "8"]}]}], ",", " ", 
     RowBox[{"q", " ", "\[Rule]", " ", 
      RowBox[{"1.602", "*", 
       SuperscriptBox["10", 
        RowBox[{"-", "19"}]]}]}]}], "}"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.7207771575734043`*^9, 3.720777213322276*^9}, 
   3.7207789261218033`*^9}],

Cell["Lege einen Ansatz fuer das E-Feld an:", "Text",
 CellChangeTimes->{{3.720777291305249*^9, 3.720777308648757*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"e", "[", 
    RowBox[{"r", ",", "t"}], "]"}], ":=", " ", 
   RowBox[{
    RowBox[{"e", "[", "r", "]"}], "*", 
    RowBox[{"Cos", "[", 
     RowBox[{"\[Omega]", "*", "t"}], "]"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.720777312309699*^9, 3.72077735663268*^9}, 
   3.720777504055522*^9, {3.7207775849991093`*^9, 3.720777598742494*^9}}],

Cell["Stelle die Wellengleichung auf:", "Text",
 CellChangeTimes->{{3.7207772508816357`*^9, 3.720777261385428*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"we", "=", 
  RowBox[{
   RowBox[{
    SuperscriptBox["c", "2"], "*", 
    RowBox[{"Laplacian", "[", 
     RowBox[{
      RowBox[{"e", "[", 
       RowBox[{"r", ",", "t"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"r", ",", "\[Theta]", ",", "z"}], "}"}], ",", 
      "\"\<Cylindrical\>\""}], "]"}]}], " ", "\[Equal]", " ", 
   RowBox[{"D", "[", 
    RowBox[{
     RowBox[{"e", "[", 
      RowBox[{"r", ",", "t"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "2"}], "}"}]}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.7207772671991177`*^9, 3.720777279037271*^9}, {
  3.720777390248559*^9, 3.720777440967866*^9}, {3.7207774739836807`*^9, 
  3.720777572807167*^9}, {3.7207776054075003`*^9, 3.720777615238701*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   SuperscriptBox["c", "2"], " ", 
   RowBox[{"(", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"Cos", "[", 
        RowBox[{"t", " ", "\[Omega]"}], "]"}], " ", 
       RowBox[{
        SuperscriptBox["e", "\[Prime]",
         MultilineFunction->None], "[", "r", "]"}]}], "r"], "+", 
     RowBox[{
      RowBox[{"Cos", "[", 
       RowBox[{"t", " ", "\[Omega]"}], "]"}], " ", 
      RowBox[{
       SuperscriptBox["e", "\[Prime]\[Prime]",
        MultilineFunction->None], "[", "r", "]"}]}]}], ")"}]}], "\[Equal]", 
  RowBox[{
   RowBox[{"-", 
    SuperscriptBox["\[Omega]", "2"]}], " ", 
   RowBox[{"Cos", "[", 
    RowBox[{"t", " ", "\[Omega]"}], "]"}], " ", 
   RowBox[{"e", "[", "r", "]"}]}]}]], "Output",
 CellChangeTimes->{{3.720777544136931*^9, 3.7207775738371973`*^9}, 
   3.720777616108781*^9, 3.720778941512588*^9, 3.72077931443958*^9, 
   3.720779969765596*^9, 3.720780595221612*^9, 3.720781786765645*^9, 
   3.720855635768358*^9, 3.720856516402377*^9, 3.720856554258052*^9, 
   3.720857203939885*^9, 3.720857533261977*^9}]
}, Open  ]],

Cell["und l\[ODoubleDot]se sie.", "Text",
 CellChangeTimes->{{3.720777447817152*^9, 3.720777450657606*^9}, {
  3.72077918876823*^9, 3.720779200658875*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"solrad", "=", 
  RowBox[{
   RowBox[{"DSolve", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"we", ",", " ", 
       RowBox[{
        RowBox[{"e", "[", "0", "]"}], "\[Equal]", " ", "e0"}]}], "}"}], ",", 
     RowBox[{"e", "[", "r", "]"}], ",", "r", ",", " ", 
     RowBox[{"Assumptions", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"r", " ", "\[LessEqual]", " ", "R"}], ",", " ", 
        RowBox[{
         RowBox[{"-", "Infinity"}], " ", "<", " ", 
         RowBox[{"e", "[", "r", "]"}], " ", "<", " ", "Infinity"}]}], 
       "}"}]}]}], "]"}], "//", "Flatten"}]}]], "Input",
 CellChangeTimes->{{3.7207777043082247`*^9, 3.720777735650525*^9}, {
  3.720777774317685*^9, 3.7207778370598288`*^9}, {3.7207778999284773`*^9, 
  3.720777903089151*^9}, {3.720777946579248*^9, 3.720777954114916*^9}, {
  3.720778256333249*^9, 3.720778318980392*^9}, {3.7207783618163137`*^9, 
  3.720778445159507*^9}, {3.720778539274529*^9, 3.720778545285589*^9}, {
  3.720778683956497*^9, 3.720778686312154*^9}, {3.7207809486712418`*^9, 
  3.720780948869522*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"e", "[", "r", "]"}], "\[Rule]", 
   RowBox[{"e0", " ", 
    RowBox[{"BesselJ", "[", 
     RowBox[{"0", ",", 
      FractionBox[
       RowBox[{"r", " ", "\[Omega]"}], "c"]}], "]"}]}]}], "}"}]], "Output",
 CellChangeTimes->{{3.720777724097722*^9, 3.720777735970379*^9}, 
   3.7207777834961853`*^9, {3.720777815470344*^9, 3.7207778397184*^9}, 
   3.7207779042027607`*^9, 3.7207779560995197`*^9, 3.720778259100583*^9, 
   3.720778321231924*^9, {3.720778369773611*^9, 3.7207784476882133`*^9}, 
   3.7207785764642143`*^9, 3.720778686825365*^9, 3.720778941866383*^9, 
   3.720779314624606*^9, 3.720779969907299*^9, 3.72078059567782*^9, 
   3.720780949538404*^9, 3.720781787133086*^9, 3.720855637035914*^9, 
   3.720856516746491*^9, 3.720856554393537*^9, 3.720857204220538*^9, 
   3.720857533429865*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"sol", " ", "=", " ", 
  RowBox[{
   RowBox[{"e", "[", 
    RowBox[{"r", ",", "t"}], "]"}], " ", "//.", " ", "solrad"}]}]], "Input",
 CellChangeTimes->{{3.7207809556767397`*^9, 3.720780976262937*^9}}],

Cell[BoxData[
 RowBox[{"e0", " ", 
  RowBox[{"BesselJ", "[", 
   RowBox[{"0", ",", 
    FractionBox[
     RowBox[{"r", " ", "\[Omega]"}], "c"]}], "]"}], " ", 
  RowBox[{"Cos", "[", 
   RowBox[{"t", " ", "\[Omega]"}], "]"}]}]], "Output",
 CellChangeTimes->{3.720780979174924*^9, 3.7207817871844673`*^9, 
  3.720855637169238*^9, 3.720856516814657*^9, 3.720856554441372*^9, 
  3.720857204286627*^9, 3.7208575334844427`*^9}]
}, Open  ]],

Cell["\<\
Nun bestimmen wir die Resonanzfrequenz aus der ersten Nullstelle,\
\>", "Text",
 CellChangeTimes->{{3.72077860136401*^9, 3.720778637948639*^9}, {
  3.720779216503581*^9, 3.7207792687583103`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"\[Omega]res", "=", 
   RowBox[{
    RowBox[{"\[Omega]", " ", "\[Rule]", " ", 
     RowBox[{
      FractionBox["c", "R"], 
      RowBox[{"BesselJZero", "[", 
       RowBox[{"0", ",", "1"}], "]"}]}]}], " ", "//.", " ", "constants"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.720778846606*^9, 3.720778920969923*^9}, {
  3.720779302894381*^9, 3.720779303094441*^9}, {3.72077935202326*^9, 
  3.7207795326530657`*^9}, {3.720779874777029*^9, 3.720779877608769*^9}, {
  3.720779908392374*^9, 3.720779926440262*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"efield", " ", "=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"e", "[", "r", "]"}], ",", "0", ",", 
    RowBox[{"e0", "*", 
     RowBox[{"Cos", "[", 
      RowBox[{"\[Omega]", "*", "t"}], "]"}]}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.720856194096078*^9, 3.720856233155897*^9}, {
  3.72085654656105*^9, 3.720856547304884*^9}, {3.7208569571248407`*^9, 
  3.720856966334091*^9}, {3.720857182294385*^9, 3.720857184514039*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"e", "[", "r", "]"}], ",", "0", ",", 
   RowBox[{"e0", " ", 
    RowBox[{"Cos", "[", 
     RowBox[{"t", " ", "\[Omega]"}], "]"}]}]}], "}"}]], "Output",
 CellChangeTimes->{
  3.720856516986909*^9, {3.720856548176489*^9, 3.720856554516526*^9}, 
   3.72085696671067*^9, 3.720857101251964*^9, {3.720857185331787*^9, 
   3.720857204466393*^9}, 3.7208575335681887`*^9}]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["Warum  ist es eine \[OpenCurlyDoubleQuote]pill box\
