 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{"b", "\[Rule]", 
    FractionBox[
     RowBox[{"2", " ", 
      RowBox[{"(", 
       RowBox[{"einj", "+", 
        RowBox[{
         SuperscriptBox["c", "2"], " ", "m", " ", "p"}]}], ")"}], " ", 
      "\[Pi]"}], 
     RowBox[{
      SuperscriptBox["c", "2"], " ", "q", " ", "thf"}]]}], "}"}], 
  "}"}]], "Output",
 CellChangeTimes->{
