 RowBox[{"g\[CapitalDelta]t", " ", "=", " ", 
  RowBox[{
   RowBox[{"\[CapitalDelta]t", " ", "\[Equal]", " ", 
    RowBox[{"\[Nu]", "*", "thf"}]}], "//.", 
   RowBox[{"\[CapitalDelta]t", "\[Rule]", " ", 
    FractionBox[
     RowBox[{"2", "*", "Pi", "*", "p", "*", "m", "*", 
      SuperscriptBox["c", "2"]}], 
     RowBox[{"q", "*", 
      SuperscriptBox["c", "2"], "*", "b"}]], " "}]}]}]], "Input",
 CellChangeTimes->{{3.7200894231338124`*^9, 3.720089452588134*^9}, 
