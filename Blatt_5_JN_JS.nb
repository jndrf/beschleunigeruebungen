Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{"gt1", " ", "//.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\[Mu]", "\[Rule]", "1"}], " ", ",", " ", 
      RowBox[{"t1", " ", "\[Rule]", " ", 
       FractionBox[
        RowBox[{"2", "*", "Pi", "*", 
         RowBox[{"(", 
          RowBox[{"einj", "+", 
           RowBox[{"p", "*", "m", "*", 
            SuperscriptBox["c", "2"]}]}], ")"}]}], 
        RowBox[{"q", "*", 
         SuperscriptBox["c", "2"], "*", "b"}]]}]}], "}"}]}], ",", " ", "b"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.720107479305521*^9, 
  3.7201075916800127`*^9}},ExpressionUUID->"8480d9d2-39ff-4b0c-a5fd-\
515367dcf95a"],

