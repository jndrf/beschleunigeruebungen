Cell["\<\
Die Leiter befindet sich auf der kreisf\[ODoubleDot]rmigen H\[UDoubleDot]lle \
des Strahlrohres. Sie sollen symmetrisch um die y=0-Ebene angeordnet sein und \
auf jeder Seite den Winkel \[Phi] in gleichen Abst\[ADoubleDot]nden einnehmen.\
\>", "Text",
 CellChangeTimes->{{3.721463659294907*^9, 3.721463692246757*^9}, 
   3.721463759471582*^9, {3.721463816823806*^9, 3.721463856477957*^9}, {
   3.721464266286551*^9, 3.721464288677051*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"multiConductorField", "[", 
   RowBox[{
   "\[Phi]max_", ",", " ", "nCond_", ",", " ", "x_", ",", " ", "y_", ",", " ",
     "R_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "\[CapitalDelta]\[Phi]", ",", " ", "\[Phi]", ",", " ", "xp", ",", " ", 
      "yp"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[CapitalDelta]\[Phi]", " ", "=", " ", 
      FractionBox[
       RowBox[{"2", "*", "\[Phi]max"}], "nCond"]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"xp", " ", ":=", " ", 
      RowBox[{"R", "*", 
       RowBox[{"Cos", "[", "\[Phi]", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"yp", " ", ":=", " ", 
      RowBox[{"R", "*", 
       RowBox[{"Sin", "[", "\[Phi]", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"B", "[", 
          RowBox[{"x", ",", " ", "y", ",", "xp", ",", " ", "yp"}], "]"}], " ",
          "+", " ", 
         RowBox[{"B", "[", 
          RowBox[{"x", ",", " ", "y", ",", " ", 
           RowBox[{"-", "xp"}], ",", " ", 
           RowBox[{"-", "yp"}]}], "]"}]}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"\[Phi]", ",", " ", 
          FractionBox[
           RowBox[{"-", " ", "\[Phi]max"}], "2"], ",", " ", 
          FractionBox["\[Phi]max", "2"], ",", " ", "\[CapitalDelta]\[Phi]"}], 
         "}"}]}], "]"}], "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.721463863119331*^9, 3.721463901030219*^9}, {
  3.721464184904335*^9, 3.721464244197755*^9}, {3.721464298500003*^9, 
