db87e740d1cd"],

Cell[BoxData[
 RowBox[{
  RowBox[{"multiConductorField", "[", 
   RowBox[{
   "\[Phi]max_", ",", " ", "nCond_", ",", " ", "x_", ",", " ", "y_", ",", " ",
     "R_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "\[CapitalDelta]\[Phi]", ",", " ", "\[Phi]", ",", " ", "xp", ",", " ", 
      "yp"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[CapitalDelta]\[Phi]", " ", "=", " ", 
      FractionBox[
       RowBox[{"2", "*", "\[Phi]max"}], "nCond"]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"xp", " ", ":=", " ", 
      RowBox[{"R", "*", 
       RowBox[{"Cos", "[", "\[Phi]", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"yp", " ", ":=", " ", 
      RowBox[{"R", "*", 
       RowBox[{"Sin", "[", "\[Phi]", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"B", "[", 
          RowBox[{"x", ",", " ", "y", ",", "xp", ",", " ", "yp"}], "]"}], " ",
          "-", 
         RowBox[{"B", "[", 
          RowBox[{"x", ",", " ", "y", ",", " ", 
           RowBox[{"-", "xp"}], ",", " ", 
           RowBox[{"-", "yp"}]}], "]"}]}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"\[Phi]", ",", " ", 
          FractionBox[
           RowBox[{"-", " ", "\[Phi]max"}], "2"], ",", " ", 
          FractionBox["\[Phi]max", "2"], ",", " ", "\[CapitalDelta]\[Phi]"}], 
         "}"}]}], "]"}], "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.721463863119331*^9, 3.721463901030219*^9}, {
